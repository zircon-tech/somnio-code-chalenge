{
  "name": "codechalenge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --config .prettierrc --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-check": "prettier --config .prettierrc --check \"src/**/*.ts\" \"test/**/*.ts\" \"envfiles/**/*.js\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db-up": "CURRENT_USER=$(id -u):$(id -g) docker-compose --file ./docker-compose.dev.yml up",
    "test:db-up": "CURRENT_USER=$(id -u):$(id -g) docker-compose --file ./docker-compose.test.yml up",
    "test:integration": "./scripts/run-integration.sh false",
    "prisma:gen": "npx prisma generate",
    "prisma:migrate": "NODE_OPTIONS=\"--require=./envfiles/load_envs.js\" prisma migrate dev --preview-feature --schema=./prisma/schema.prisma --skip-seed",
    "prisma:deploy": "NODE_OPTIONS=\"--require=./envfiles/load_envs.js\" prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:seed": "NODE_OPTIONS=\"--require=./envfiles/load_envs.js\" prisma db seed --schema=./prisma/schema.prisma"
  },
  "engines": {
    "node": ">=22.5.1 <23",
    "npm": ">=10.8.2 <11"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "openai": "^4.72.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
